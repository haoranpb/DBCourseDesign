        $(document).ready(function ()
        {
            //代码里面因为后端controller里面有些地方需要改，所以前端代码有的地方需要根据相应的注释来改
            
            /*---------------------加载店铺---------------------*/
            //卖家的店铺string
            var shopidstring = "@ViewBag.shopidstring";
            //卖家的店铺list
            var shoplist = shopidstring.split(" ");
            for (var i = 1; i < shoplist.length; i++) {
                var shop_url = "../Saler/ShopInfo?shopid=" + shoplist[i];
                $.get(shop_url, function (data)
                {
                    //没有第i个店铺，设置为新店铺，可添加店铺添加商品
                    if (data == "shop not exist") {
                        $("#shop" + i + "outer").text("New Shop");
                        $("#shop" + i + "v").text("Add");
                    }
                    //有第i个店铺，显示店铺名
                    else if (data == "success") {
                        $("#shop" + i + "outer").text("@ViewBag.ShopName");
                    }
                })
            }
            /*---------------------加载订单---------------------*/
            var order_url = "../Saler/Order?shopid1=" + shoplist[1] + "&shopid2=" + shoplist[2] + "&shopid3=" + shoplist[3];
            $.get(order_url, function (data) {
                //订单string
                var orderstr = data;
                //订单list
                var orderlist = orderstr.split(" ");
                for (var i = 1; i <= 3; i++) {
                    //获取订单信息
                    var orderinfo_url = "../Saler/Orderinfo?Orderid=" + orderlist[i];
                    $.get(orderinfo_url, function (data) {
                        //解析JSON
                        var orderinfo = eval(data);

                        //需要额外的属性 Controller里JSON需要添加数据
                        var orderid = orderinfo.OrderId;
                        var ordertime = orderinfo.OrderTime;
                        var shopid = orderinfo.ShopId;
                        var orderphone = orderinfo.OrderPhone;
                        var orderaddress = orderinfo.OrderAddress;
                        var orderprice = orderinfo.OrderPrice;
                        var orderstatus = orderinfo.OrderStatus;

                        //显示的订单信息
                        $("#order" + i + "id").text(orderid);
                        $("#order" + i + "time").text(ordertime);
                        $("#order" + i + "shop").text(shopid);
                        //修改订单弹出页面的信息 提前加载
                        $("#eprice" + i).attr("value", orderprice);
                        $("#ephone" + i).attr("value", orderphone);
                        $("#eaddress" + i).attr("value", orderaddress);

                        //如果订单状态为已完成，不可使用Edit和Finish按钮
                        //我不知道订单状态回返回什么，我这里用的是finished，要根据实际数据修改
                        if (orderstatus == "finished") {
                            $("#editorder" + i).attr("disable", true);
                            $("#finish" + i).attr("disable", true);
                        }
                    })
                }
            });
            /*---------------------查看店铺---------------------*/
            $("button").click(function () {
                //获得当前点击的按钮id
                var id = $(this).attr("id");
                //获得内容 view为查看店铺 add为添加店铺
                var type = $(this).text();
                //获得id中的数字即店铺 1-3
                var num = $(this).attr("id").replace(/[^0-9]/ig, "");

                if (type == "View") {
                    var shopinfo_url = "../Saler/ShopInfo?shopid=" + shoplist[num];
                    $.get(shopinfo_url, function (data) {
                        $("#shopn").text(data);
                        $("#salern").text("@ViewBag.SalerName");
                        $("#salercr").text("@ViewBag.SalerCredit");
                    });
                }
            });
            /*---------------------添加店铺---------------------*/
            $("button").click(function () {
                //获得当前点击的按钮id
                var id = $(this).attr("id");
                //获得内容 view为查看店铺 add为添加店铺
                var type = $(this).text();
                //获得id中的数字即店铺 1-3
                var num = $(this).attr("id").replace(/[^0-9]/ig, "");

                if (type == "Add") {
                    var shopname = $("#nshopn").val();
                    var addshop_url = "../Saler/CreateShop?ShopName=" + shopname + "&SalerCredit=@ViewBag.SalerCredit&SalerName=@ViewBag.SalerName";
                    $.get(addshop_url, function (data) {
                        if (data == "failure") {
                            alert("Failed to Add a New Shop");
                        }
                        else {
                            //Controller里面好像是重定向了，那这里应该就不用做了吧我也不知道
                        }
                    })
                }
            })
            /*---------------------查看商品---------------------*/
            $("button").click(function () {
                //获得当前点击的按钮id
                var id = $(this).attr("id");
                //获得内容 Products则定向到商品页面
                var type = $(this).text();
                if (type == "Products") {
                    //重定向到allproducts那个页面 不会写url噗
                    $(window).attr('location', '..');
                }
            })
            /*---------------------编辑订单---------------------*/
            $("button").click(function () {
                //获得当前点击的按钮id
                var id = $(this).attr("id");
                //获得内容 econfirm则提交编辑订单 就是点击edit之后弹出窗口里面的那个Confirm
                var type = $(this).text();
                type = type.substr(0, type.length - 1);
                //获得id中的数字即订单编号 1-3
                var num = $(this).attr("id").replace(/[^0-9]/ig, "");

                if (type == "ecomfirm") {
                    var price = $("#eprice" + num).val();
                    var phone = $("#ephone" + num).val();
                    var address = $("eaddress" + num).val();

                    //controller里面没有用来修改order的API哇😭
                    //我在controller里面也加了注释了😭
                }
            })
            /*---------------------完成订单---------------------*/
            $("button").click(function () {
                //获得当前点击的按钮id
                var id = $(this).attr("id");
                //获得内容 finish则完成订单 修改状态
                var type = $(this).text();
                type = type.substr(0, type.length - 1);
                //获得id中的数字即订单编号 1-3
                var num = $(this).attr("id").replace(/[^0-9]/ig, "");

                if (type == "finish") {
                    //获取订单id 可以用在之后向Controller传递数据
                    var orderid = $("#order" + num + "id").text();

                    //我发现Controller里面还是没有API😭

                    //修改按钮状态 订单完成之后 Edit和Finish按钮不可用
                    //如果后端函数里进行重定向，那么可以删掉这两行 因为重定向刷新之后会动态加载
                    $(this).attr("disable", true);
                    $("#editorder" + num).attr("disable", true);
                }
            })
        });
