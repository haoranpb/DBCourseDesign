                        $("#name").attr("value", name);		//表单商品名称
                        $("#price").attr("value", price);	//表单商品价格
                    });

                    /*---------------------提交修改---------------------*/
                    $("#confirm").click(function () {
                        var picname = $("#picname").val();
                        var picpath = $("#picpath").val();
                        var name = $("#name").val();
                        var price = $("#price").val();
