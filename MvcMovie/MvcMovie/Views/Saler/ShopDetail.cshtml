                        var name = $("#name").val();
                        var price = $("#price").val();
                        
                        var modify_url = "/Shop/Additem?ItemId=" + itemid + "&ItemInfo=" + name + "&ItemName=" + name + "&ItemPic=" + picname + "&PATH=" + picpath + "&ItemPrice=" + price + "&ItemRemain=500&ShopID=" + shopid;
