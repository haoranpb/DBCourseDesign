                            if (data == "success") {
                                alert("Edited Successfully");
                                //刷新 跳转到当前页面 url不知道
                                window.location.reload();
                            }
                            else {
                                alert("Failed to Edit");
                            }
                        });
                    });
                }
                else if(type=="a"){
                    $("#confirm").click(function () {
                        var picname = $("#picname").val();
