                        var itemObj = eval(itemjson);

                        var id = itemObj.ItemID;
                        var info = itemObj.ItemInfo;
                        var name = itemObj.ItemName;
                        var pic = itemObj.ItemPic;
                        var price = itemObj.ItemPrice;

                        $("#picname").attr("value", pic);		//表单图片名称
